{
    "name": "johndev/hermes",
    "description": "A simple message broker package",
    "keywords": [
        "amqp", "sqs", "message broker"
    ],
    "support": {
        "issues": "https://github.com/jortiz3109/hermes/issues",
        "source": "https://github.com/jortiz3109/hermes/"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "John Edisson Ortiz",
            "email": "me@johndev.co"
        }
    ],
    "require": {
        "php": ">=7.4",
        "bschmitt/laravel-amqp": "^2.0",
        "illuminate/config": "^8.50",
        "illuminate/console": "^8.50"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "Hermes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hermes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hermes\\Providers\\HermesServiceProvider"
            ],
            "aliases": {
                "Hermes": "Hermes"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
